    <PackageReference Include="System.ServiceModel.Http">
      <Version>4.7.0</Version>
    </PackageReference>
    <PackageReference Include="System.ServiceModel.Primitives">
      <Version>4.7.0</Version>
    </PackageReference>
  </ItemGroup>-->

  <ItemGroup>
    <Reference Include="System.ServiceModel" />
    <Reference Include="System.Web.Services" />
  </ItemGroup>

</Project>